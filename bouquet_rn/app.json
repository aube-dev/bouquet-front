{
  "expo": {
    "name": "Bouquet",
    "scheme": "bouquet",
    "slug": "bouquet",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/png/icon.png",
    "splash": {
      "image": "./assets/png/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "notification": {
      "icon": "./assets/png/icon.png"
    },
    "ios": {
      "supportsTablet": false,
      "usesAppleSignIn": false,
      "bundleIdentifier": "com.dot.bouquet",
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"]
      },
      "buildNumber": "2",
      "icon": "./assets/png/icon.png"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/png/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.dot.bouquet"
    },
    "web": {
      "favicon": "./assets/png/favicon.png"
    }
  }
}
